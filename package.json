{
    "name": "carved-react",
    "description": "Carved Design User Interface Components for React",
    "version": "0.2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/plurid/carved-react.git"
    },
    "author": "Jan Cavel <mail@caveljan.com> (https://caveljan.com/)",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dist": "yarn dist:babel && yarn dist:types",
        "dist:babel": "rm -rf dist && NODE_ENV=production babel src/lib --extensions .ts,.tsx --out-dir dist --copy-files --source-maps inline",
        "dist:types": "tsc src/lib/index --declaration true --jsx preserve --emitDeclarationOnly --outDir dist/types",
        "start:examples": "react-scripts start",
        "build:examples": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "node-sass": "^4.11.0",
        "react": "^16.8.1",
        "react-dom": "^16.8.1",
        "react-scripts": "2.1.3",
        "styled-components": "^4.1.3"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.7",
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@types/jest": "^24.0.0",
        "@babel/plugin-proposal-class-properties": "^7.3.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-typescript": "^7.1.0",
        "@types/react": "^16.8.2",
        "@types/react-dom": "^16.8.0",
        "@types/react-test-renderer": "^16.8.0",
        "@types/styled-components": "^4.1.8",
        "@types/uuid": "^3.4.4",
        "babel-plugin-styled-components": "^1.10.0",
        "react-test-renderer": "^16.8.1",
        "typescript": "^3.3.1",
        "uuid": "^3.3.2"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
